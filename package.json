{
  "name": "@nest-me-up/redis-modules",
  "version": "1.0.3",
  "description": "Library for NestJS Redis based modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "ItaiH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nest-me-up/redis-modules.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "test": "service_config='src/config/test.yml' jest --passWithNoTests --detectOpenHandles --forceExit",
    "test:ci": "service_config='src/config/ci.yml' jest --passWithNoTests --forceExit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:component": "service_config='src/utils/config/test.yml' jest --config test/component/config/jest-component-test.json --runInBand --forceExit --detectOpenHandles",
    "test:component-ci": "service_config='src/utils/config/ci.yml' jest --config test/component/config/jest-component-test.json --runInBand --forceExit"
  },
  "peerDependencies": {
    "@nest-me-up/common": ">=1.0.0",
    "ioredis": ">=5.0.0",
    "uuid": ">=9.0.0"
  },
  "devDependencies": {
    "@nest-me-up/common": "1.0.14",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/testing": "^11.1.11",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "uuid": "^11.1.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
